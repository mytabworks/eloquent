{
  "name": "eloquents",
  "version": "0.1.1",
  "description": "it is a object relational mapping for typeorm extension",
  "main": "dist/index.js",
  "source": "src/eloquent/index.ts",
  "targets": {
    "node": {
      "includeNodeModules": {
        "typeorm": false
      }
    }
  },
  "scripts": {
    "build": "parcel build src/eloquent/index.ts --no-cache --target node --bundle-node-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mytabworks/eloquent.git"
  },
  "keywords": [
    "ORM"
  ],
  "author": "mytabworks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mytabworks/eloquent/issues"
  },
  "homepage": "https://github.com/mytabworks/eloquent#readme",
  "dependencies": {
    "illusionjs": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.5.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "typeorm": "^0.2.30"
  }
}
